	СЦентр_УправлениеРемонтамиКлиент.ПроверитьКоличествоЕдиницуИзмеренияИнвентарногоТовара(Элементы.Отходы.ТекущиеДанные);
	
КонецПроцедуры

&НаКлиенте
Процедура РедактироватьСерийныеНомера(Команда)
	
	СЦентр_УправлениеРемонтамиКлиент.РедактироватьСерийныеНомера(Объект, ЭтаФорма, Элементы.Запасы.ТекущиеДанные);
	
КонецПроцедуры

&НаКлиенте
Процедура РедактироватьСерийныеНомераПродукция(Команда)
	
	СЦентр_УправлениеРемонтамиКлиент.РедактироватьСерийныеНомера(Объект, ЭтаФорма, Элементы.Продукция.ТекущиеДанные);
	
КонецПроцедуры

&НаКлиенте
// Процедура Подменяет параметры для проверки указания серийных номеров
//
Функция ИзменитьПараметрыОтВидаОперации(Продукция = Ложь)
	
	НовыеПараметры = Новый Структура();
	
	Для Каждого Параметр Из ПараметрыУказанияСерийныхНомеров Цикл
		
		НовыеПараметры.Вставить(Параметр.Ключ, Параметр.Значение);
		
	КонецЦикла;
	
	НовыеПараметры.Удалить("СтруктурныеЕдиницы");
	
	Если Объект.ВидОперации = ПредопределенноеЗначение("Перечисление.ВидыОперацийСборкаЗапасов.Сборка") Тогда
		
		Если Продукция Тогда
			
			НовыеПараметры.Вставить("ИмяРеквизитаСтруктурнаяЕдиница", "СтруктурнаяЕдиницаПродукции");
			НовыеПараметры.Вставить("ХозОперация", ПредопределенноеЗначение("Перечисление.СЦентр_ХозОперацииДвиженияСерийныхНомеров.ПриемкаЗапасовПослеСборки"));
			
		Иначе
			
			НовыеПараметры.Вставить("ИмяРеквизитаСтруктурнаяЕдиница", "СтруктурнаяЕдиницаЗапасов");
			НовыеПараметры.Вставить("ХозОперация", ПредопределенноеЗначение("Перечисление.СЦентр_ХозОперацииДвиженияСерийныхНомеров.ОтгрузкаКомплектующихДляСборки"));
			
		КонецЕсли;
		
	Иначе
		
		Если Продукция Тогда
			
			НовыеПараметры.Вставить("ИмяРеквизитаСтруктурнаяЕдиница", "СтруктурнаяЕдиницаЗапасов");
			НовыеПараметры.Вставить("ХозОперация", ПредопределенноеЗначение("Перечисление.СЦентр_ХозОперацииДвиженияСерийныхНомеров.ОтгрузкаЗапасовДляРазборки"));
			
		Иначе
			
			НовыеПараметры.Вставить("ИмяРеквизитаСтруктурнаяЕдиница", "СтруктурнаяЕдиницаПродукции");
			НовыеПараметры.Вставить("ХозОперация", ПредопределенноеЗначение("Перечисление.СЦентр_ХозОперацииДвиженияСерийныхНомеров.ПриемкаКомплектующихПослеРазборки"));
			
		КонецЕсли;
		
	КонецЕсли;
	
	Возврат НовыеПараметры;
	
КонецФункции

// Процедура формирует соответствие видов операций.
//
&НаСервере
Процедура ПолучитьСтруктуруВидовОпераций()
	
	Структура = Новый Структура;
	Структура.Вставить("Сборка", Перечисления.ВидыОперацийСборкаЗапасов.Сборка);
	Структура.Вставить("Разборка", Перечисления.ВидыОперацийСборкаЗапасов.Разборка);
	
	ВидыОпераций = Структура;
	
КонецПроцедуры // ПолучитьСтруктуруВидовОпераций()

#КонецОбласти

&НаКлиенте
Процедура ГенерацияШК(Команда)
	
	Массив = Новый Массив;
	Массив.Добавить(Тип("СправочникСсылка.Номенклатура"));
	
	ОписаниеТиповК = Новый ОписаниеТипов(Массив);
	
	номенклатура="";
	
	if not ВвестиЗначение(номенклатура, "для чего будут характеристики", ОписаниеТиповК) then return endif;
	
	Массив = Новый Массив;
	Массив.Добавить(Тип("СправочникСсылка.Спецификации"));
	
	ОписаниеТиповК = Новый ОписаниеТипов(Массив);
	
	Спецификация="";
	
	if not ВвестиЗначение(Спецификация, "спецификация", ОписаниеТиповК) then return endif;
	
	префикс="vin.omnic.net/";
	
	if not ВвестиСтроку(префикс,"префикс ШК") then return endif;;
	
	начштрихчисло=1;
	if not ВвестиЧисло(начштрихчисло,"начальное значение числовой части ШК") then return endif;
	
	штрихкоддлина=6;
	if not ВвестиЧисло(штрихкоддлина,"длина числовой части ШК") then return endif;
	
	количество=1;
	if not ВвестиЧисло(количество,"количество ШК",3) then return endif;
	СоздатьСтрокиШК(количество,номенклатура,штрихкоддлина,префикс,начштрихчисло,спецификация);
	
КонецПроцедуры
//можно вынести в библиотеку
&НаСервере
function СоздатьСтрокиШК(количество,номенклатура,штрихкоддлина,префикс,начштрихчисло,спецификация)
	
	док=РеквизитФормыВЗначение("Объект");
	док.Продукция.очистить();
	текштрих=стрзаменить(строка(начштрихчисло),символы.нпп,"" );
	for сч=1 to количество do
		текштрих=стрзаменить(текштрих,символы.нпп,"" );
		if (штрихкоддлина-СтрДлина(текштрих)>0) then
			нолей="";
			for i=1 по (штрихкоддлина-СтрДлина(текштрих)) do
			нолей=нолей+"0";	
			enddo;	
		endif;	
		штрихкод=префикс+нолей+текштрих;
		
		продукт=док.Продукция.добавить();
		продукт.Номенклатура=номенклатура;
		продукт.количество=1;
		продукт.Спецификация=спецификация;
		
		ТекущаяХарактеристика=Справочники.ХарактеристикиНоменклатуры.СоздатьЭлемент();
		ТекущаяХарактеристика.Наименование=штрихкод;
		НаименованиеДляПечати= штрихкод;
		ТекущаяХарактеристика.Владелец=номенклатура;
		ТекущаяХарактеристика.Записать();
		продукт.характеристика=ТекущаяХарактеристика.Ссылка;
		//
		НаборЗаписей=РегистрыСведений.ШтрихкодыНоменклатуры.СоздатьНаборЗаписей();
		НаборЗаписей.Отбор.Штрихкод.Установить(штрихкод);
		НаборЗаписей.Очистить();
		НоваяЗапись=НаборЗаписей.Добавить();
		НоваяЗапись.Штрихкод		= штрихкод;
		НоваяЗапись.Номенклатура	= Номенклатура;
		НоваяЗапись.Характеристика	= ТекущаяХарактеристика.Ссылка;		
		НаборЗаписей.Записать();
		текштрих=число(текштрих)+1;
	enddo;
	
	ЗначениеВРеквизитФормы(док,"Объект");
	
endfunction

&НаКлиенте
Процедура ГенерацияШКЗапасы(Команда)
	
	Массив = Новый Массив;
	Массив.Добавить(Тип("СправочникСсылка.Номенклатура"));
	
	ОписаниеТиповК = Новый ОписаниеТипов(Массив);
	
	номенклатура="";
	
	if not ВвестиЗначение(номенклатура, "для чего будут характеристики", ОписаниеТиповК) then return endif;
	
	префикс="vin.omnic.net/";
	
	if not ВвестиСтроку(префикс,"префикс ШК") then return endif;;
	
	начштрихчисло=1;
	if not ВвестиЧисло(начштрихчисло,"начальное значение числовой части ШК") then return endif;
	
	штрихкоддлина=6;
	if not ВвестиЧисло(штрихкоддлина,"длина числовой части ШК") then return endif;
	
	количество=1;
	if not ВвестиЧисло(количество,"количество ШК",3) then return endif;
	СоздатьСтрокиШКЗапасы(количество,номенклатура,штрихкоддлина,префикс,начштрихчисло);
	
КонецПроцедуры

&НаСервере
function СоздатьСтрокиШКЗапасы(количество,номенклатура,штрихкоддлина,префикс,начштрихчисло)
	
	док=РеквизитФормыВЗначение("Объект");
	док.Продукция.очистить();
	текштрих=стрзаменить(строка(начштрихчисло),символы.нпп,"" );
	for сч=1 to количество do
		текштрих=стрзаменить(текштрих,символы.нпп,"" );
		if (штрихкоддлина-СтрДлина(текштрих)>0) then
			нолей="";
			for i=1 по (штрихкоддлина-СтрДлина(текштрих)) do
			нолей=нолей+"0";	
			enddo;	
		endif;	
		штрихкод=префикс+нолей+текштрих;
		
		Запас=док.Запасы.добавить();
		Запас.Номенклатура=номенклатура;
		Запас.количество=1;
		
		ТекущаяХарактеристика=Справочники.ХарактеристикиНоменклатуры.СоздатьЭлемент();
		ТекущаяХарактеристика.Наименование=штрихкод;
		НаименованиеДляПечати= штрихкод;
		ТекущаяХарактеристика.Владелец=номенклатура;
		ТекущаяХарактеристика.Записать();
		Запас.характеристика=ТекущаяХарактеристика.Ссылка;
		//
		НаборЗаписей=РегистрыСведений.ШтрихкодыНоменклатуры.СоздатьНаборЗаписей();
		НаборЗаписей.Отбор.Штрихкод.Установить(штрихкод);
		НаборЗаписей.Очистить();
		НоваяЗапись=НаборЗаписей.Добавить();
		НоваяЗапись.Штрихкод		= штрихкод;
		НоваяЗапись.Номенклатура	= Номенклатура;
		НоваяЗапись.Характеристика	= ТекущаяХарактеристика.Ссылка;		
		НаборЗаписей.Записать();
		текштрих=число(текштрих)+1;
	enddo;
	
	ЗначениеВРеквизитФормы(док,"Объект");
	
endfunction

